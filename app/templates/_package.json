{
  "name": "<%= appName %>",
  "productName": "<%= classifiedAppName %>",
  "version": "0.0.0",
  "description": "My <%= superb %> app",
  "license": "MIT",
  "main": "index.js",
  "repository": "<%= githubUsername %>/<%= appName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . $npm_package_productName --out=$npm_package_productName-dist --ignore=$npm_package_productName-dist --prune --asar --all --version=0.30.0"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "electron-debug": "^0.1.1"
  },
  "devDependencies": {
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.30.0"
  }
}
